{
  "name": "fork-version",
  "version": "1.4.45",
  "license": "ISC",
  "description": "Replacement for standard version written with modern syntax",
  "homepage": "https://github.com/eglavin/fork-version",
  "bugs": {
    "url": "https://github.com/eglavin/fork-version/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eglavin/fork-version.git"
  },
  "author": {
    "name": "Eanna Glavin",
    "url": "https://eglavin.com"
  },
  "packageManager": "pnpm@8.15.6",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs",
      "types": "./dist/cli.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "fork-version": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "fork-version": "node dist/cli.js",
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint --fix \"src/**/*.ts\"",
    "lint:check": "eslint \"src/**/*.ts\"",
    "lint:format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:cover": "vitest --coverage",
    "test:ci": "vitest run --coverage --no-file-parallelism",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "bundle-require": "4.0.2",
    "conventional-changelog": "5.1.0",
    "conventional-changelog-config-spec": "2.1.0",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "conventional-recommended-bump": "9.0.0",
    "detect-indent": "7.0.1",
    "detect-newline": "4.0.1",
    "esbuild": "0.20.2",
    "git-semver-tags": "7.0.1",
    "joycon": "3.1.1",
    "meow": "13.2.0",
    "semver": "7.6.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/git-raw-commits": "2.0.4",
    "@types/json-schema": "7.0.15",
    "@types/node": "20.11.30",
    "@types/semver": "7.5.8",
    "@vitest/coverage-v8": "1.4.0",
    "@vitest/ui": "1.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "globals": "15.0.0",
    "prettier": "3.2.5",
    "tsup": "8.0.2",
    "typescript": "5.4.3",
    "typescript-eslint": "7.4.0",
    "vitest": "1.4.0"
  }
}